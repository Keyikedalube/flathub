{
	"app-id": "io.itch.keyikedalube_ndang.Type-Off",
	"runtime": "org.freedesktop.Platform",
	"runtime-version": "19.08",
	"sdk": "org.freedesktop.Sdk",
	"command": "Type Off.x86_64",
	"finish-args": [
		"--socket=x11",
		"--socket=pulseaudio",
		"--device=dri"
	],
	"modules": [
		{
			"name": "Type-Off-pack-file",
			"buildsystem": "simple",
			"build-commands": [
				"install -D \"Type Off.pck\" /app/bin/\"Type Off.pck\""
			],
			"sources": [
				{
					"type": "file",
					"path": "Type Off.pck"
				}
			]
		},
		{
			"name": "Type-Off",
			"buildsystem": "simple",
			"build-commands": [
				"install -D \"Type Off.x86_64\" /app/bin/\"Type Off.x86_64\""
			],
			"sources": [
				{
					"type": "file",
					"path": "Type Off.x86_64"
				}
			]
	
		}
	]
}
